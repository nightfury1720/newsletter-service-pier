{
  "name": "newsletter-service",
  "version": "1.0.0",
  "description": "Newsletter Service with Queue and Connection Pooling",
  "main": "dist/app.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node ./dist/app.js",
    "dev": "tsx watch src/app.ts",
    "test:data": "tsx src/utils/testData.ts",
    "test:data:simple": "tsx src/utils/testDataSimple.ts",
    "test:redis": "tsx test-redis-connection.ts",
    "type-check": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx src/utils/migrate.ts",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx src/utils/seedMockData.ts"
  },
  "keywords": [
    "newsletter",
    "email",
    "scheduler"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bull": "^4.12.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.31.7",
    "drizzle-orm": "^0.44.7",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "postgres": "^3.4.7",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.10.9",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
